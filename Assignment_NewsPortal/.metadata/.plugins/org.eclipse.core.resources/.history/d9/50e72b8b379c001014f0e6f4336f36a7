package com.newsportal.model;

public class Category {
	public int Id;
	public String Name;
	public Category(int id, String name) {
		super();
		Id = id;
		Name = name;
	}
	public Category() {
		super();
	}
	public int getId() {
		return Id;
	}
	public void setId(int id) {
		Id = id;
	}
	public String getName() {
		return Name;
	}
	public void setName(String name) {
		Name = name;
	}
	
	
}
