package com.newsportal.model;

public class Category {
	public int id;
	public String name;
	public Category(int id, String name) {
		super();
		id = id;
		name = name;
	}
	public Category() {
		super();
	}
	public int getId() {
		return id;
	}
	public void setId(int id) {
		id = id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		name = name;
	}
	
	
}
